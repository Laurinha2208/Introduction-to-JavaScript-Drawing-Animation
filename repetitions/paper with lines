for (var i = 0;i < 20;i++) {
    var lineY = 20 + (i * 20);
    line(0, lineY, 400, lineY);
}
for (var j = 0;j < 20;j++) {
    var linex = 20 + (j * 20);
    line(linex, 400, linex, 0);
}

